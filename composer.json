{
    "name": "smmahfujurrahman/docgen",
    "description": "A beautiful Laravel package for viewing Markdown documentation with GitHub-style rendering",
    "type": "library",
    "keywords": [
        "laravel",
        "documentation",
        "markdown",
        "docs",
        "viewer",
        "ajmain"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "S. M. Mahfujur Rahman",
            "email": "mahfujur.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "league/commonmark": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ajmain\\DocGen\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ajmain\\DocGen\\DocGenServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
